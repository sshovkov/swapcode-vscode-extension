{
  "name": "swapcode",
  "displayName": "Swap Code",
  "description": "Fast and easy keybinding to swap text",
  "icon": "assets/swapcode-icon.png",
  "publisher": "SophiaShovkovy",
  "version": "1.0.3",
  "pricing": "Free",
  "keywords": [
    "swap",
    "swap text",
    "swap code",
    "rearrange",
    "organize code",
    "swap text shortcut"
  ],
  "bugs": {
    "url": "https://github.com/sshovkov/swaptext-vscode-extension/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/sshovkov/swaptext-vscode-extension"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "swaptext.swap",
        "key": "ctrl+shift+v",
        "mac": "cmd+shift+v",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "swaptext.swap",
        "title": "Swap Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/glob-stream": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.7",
    "glob-stream": "^8.0.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  }
}